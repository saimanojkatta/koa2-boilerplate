    "koa-bodyparser": "^2.3.0",
    "koa-json": "^1.1.3",
    "koa-logger": "^1.3.0",
    "koa-route": "^2.4.2",
    "koa-router": "^5.4.0"
  },
  "devDependencies": {
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
