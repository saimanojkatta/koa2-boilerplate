{
  "name": "koa2-boilerplate",
  "version": "1.0.0",
  "description": "Minimal koa v2 boilerplate.",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-register --harmony --reporter spec --require should test/*.js",
    "start": "./node_modules/.bin/nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geekplux/koa2-boilerplate.git"
